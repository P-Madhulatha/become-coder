#second largest number
n=int(input())                     # 2 1 3 4 5 6 4 3 2
data=list(map(int,input().split()))#[2,1,3,4,5,6,4,3,2]
k=list(set(data))                  #[2,1,3,4,5,6]
k.sort()                           #[1,2,3,4,5,6]
print(k)                           #[1,2,3,4,5,6]
print(k[-2])                       #5
